# Edit the user section with your information.
##

[user]
	name = Vagrant 
	email = vagrant@example.com

[color]
	ui = auto

[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green

[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold

[color "status"]
	added = yellow
	changed = green
	untracked = cyan

[alias]
	st = status -sb # for short, branch info
	ci = commit
	br = branch
	co = checkout
	df = diff -w 
	lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
	lgg = log --graph --pretty=format:\"%Cred%h%Creset %C(yellow)%d%Creset - %s\" --all
	lol = log --graph --decorate --pretty=oneline --abbrev-commit
	lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
	logs = log --stat
	ls = ls-files

[push]
	default = simple
